<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="594.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Administradores.TablaEmpleadosController">
    <children>
        <Label layoutX="30.0" layoutY="30.0" text="Empleados" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>

        <TableView fx:id="tablaCuentas" layoutX="31.2" layoutY="80.0" prefHeight="391.0" prefWidth="689.0" AnchorPane.bottomAnchor="79.0" AnchorPane.leftAnchor="31.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="80.0">
            <columns>
                <TableColumn fx:id="colId" prefWidth="150.0" text="ID" />

                <TableColumn fx:id="colNombre" prefWidth="150.0" text="Nombre" />

                <TableColumn fx:id="colEmail" prefWidth="120.0" text="Email" />
            <TableColumn fx:id="colContraseña" prefWidth="75.0" text="Contraseña" />
            <TableColumn fx:id="colWorkerId" prefWidth="75.0" text="Worker ID" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
      <Button fx:id="btnModificarEmplado" layoutX="31.0" layoutY="537.0" mnemonicParsing="false" onAction="#onModificarEmpleado" text="Modificar" />
      <Button fx:id="btnBorrar" layoutX="169.0" layoutY="537.0" mnemonicParsing="false" onAction="#onBorrarEmpleado" text="Borrar" />

    </children>
</AnchorPane>
